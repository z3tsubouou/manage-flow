{
	"productName": "manage-flow",
	"version": "0.1.0",
	"identifier": "com.manage-flow.app",
	"build": {
		"beforeDevCommand": "yarn dev",
		"devUrl": "http://localhost:1420",
		"beforeBuildCommand": "yarn build",
		"frontendDist": "../dist"
	},
	"app": {
		"windows": [
			{
				"title": "manage-flow",
				"width": 1000,
				"height": 600
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"]
	}
}
